{
  "compilerOptions": {
    "target": "es6",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./build"
  },
  "lib": ["es2015"],
  "exclude": ["node_modules"]
}
